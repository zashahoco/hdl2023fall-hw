#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x560f8d822e40 .scope module, "adder2_tb" "adder2_tb" 2 3;
 .timescale 0 0;
v0x560f8d837320_0 .net "c", 0 0, L_0x560f8d837680;  1 drivers
v0x560f8d8373e0_0 .var "x", 1 0;
v0x560f8d8374b0_0 .var "y", 1 0;
v0x560f8d8375b0_0 .net "z", 1 0, L_0x560f8d837720;  1 drivers
S_0x560f8d822fc0 .scope module, "u1" "adder2" 2 9, 3 1 0, S_0x560f8d822e40;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "a"
    .port_info 1 /INPUT 2 "b"
    .port_info 2 /OUTPUT 2 "sum"
    .port_info 3 /OUTPUT 1 "carry"
L_0x7fa8b627b060 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x560f8d8022e0_0 .net *"_s10", 0 0, L_0x7fa8b627b060;  1 drivers
v0x560f8d836b60_0 .net *"_s11", 2 0, L_0x560f8d837b10;  1 drivers
v0x560f8d836c40_0 .net *"_s3", 2 0, L_0x560f8d837860;  1 drivers
L_0x7fa8b627b018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x560f8d836d30_0 .net *"_s6", 0 0, L_0x7fa8b627b018;  1 drivers
v0x560f8d836e10_0 .net *"_s7", 2 0, L_0x560f8d8379a0;  1 drivers
v0x560f8d836f40_0 .net "a", 1 0, v0x560f8d8373e0_0;  1 drivers
v0x560f8d837020_0 .net "b", 1 0, v0x560f8d8374b0_0;  1 drivers
v0x560f8d837100_0 .net "carry", 0 0, L_0x560f8d837680;  alias, 1 drivers
v0x560f8d8371c0_0 .net "sum", 1 0, L_0x560f8d837720;  alias, 1 drivers
L_0x560f8d837680 .part L_0x560f8d837b10, 2, 1;
L_0x560f8d837720 .part L_0x560f8d837b10, 0, 2;
L_0x560f8d837860 .concat [ 2 1 0 0], v0x560f8d8373e0_0, L_0x7fa8b627b018;
L_0x560f8d8379a0 .concat [ 2 1 0 0], v0x560f8d8374b0_0, L_0x7fa8b627b060;
L_0x560f8d837b10 .arith/sum 3, L_0x560f8d837860, L_0x560f8d8379a0;
    .scope S_0x560f8d822e40;
T_0 ;
    %vpi_call 2 12 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars" {0 0 0};
    %vpi_call 2 14 "$monitor", "%t %b %b %b %b", $time, v0x560f8d8373e0_0, v0x560f8d8374b0_0, v0x560f8d8375b0_0, v0x560f8d837320_0 {0 0 0};
    %pushi/vec4 50, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 16 "$random" 32 {0 0 0};
    %pad/u 4;
    %split/vec4 2;
    %store/vec4 v0x560f8d8374b0_0, 0, 2;
    %store/vec4 v0x560f8d8373e0_0, 0, 2;
    %delay 100, 0;
    %load/vec4 v0x560f8d837320_0;
    %load/vec4 v0x560f8d8375b0_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x560f8d8373e0_0;
    %pad/u 3;
    %load/vec4 v0x560f8d8374b0_0;
    %pad/u 3;
    %add;
    %cmp/ne;
    %jmp/0xz  T_0.2, 6;
    %vpi_call 2 19 "$display", "Error: %b + %b = %b %b", $time, v0x560f8d8373e0_0, v0x560f8d8374b0_0, v0x560f8d8375b0_0, v0x560f8d837320_0 {0 0 0};
    %vpi_call 2 20 "$finish", 32'sb00000000000000000000000000000001 {0 0 0};
T_0.2 ;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %delay 100, 0;
    %vpi_call 2 25 "$finish", 32'sb00000000000000000000000000000000 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
