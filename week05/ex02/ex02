#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55b677ae17f0 .scope module, "isodd_tb" "isodd_tb" 2 3;
 .timescale 0 0;
v0x55b677af2b80_0 .var "a", 3 0;
v0x55b677af2c40_0 .net "x", 0 0, L_0x55b677af2d10;  1 drivers
S_0x55b677ae1970 .scope module, "uut" "isodd" 2 8, 3 3 0, S_0x55b677ae17f0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /OUTPUT 1 "x"
v0x55b677aac100_0 .net "a", 3 0, v0x55b677af2b80_0;  1 drivers
v0x55b677af2a60_0 .net "x", 0 0, L_0x55b677af2d10;  alias, 1 drivers
L_0x55b677af2d10 .part v0x55b677af2b80_0, 0, 1;
    .scope S_0x55b677ae17f0;
T_0 ;
    %vpi_call 2 14 "$dumpfile", "isodd_tb.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars" {0 0 0};
    %vpi_call 2 16 "$monitor", "%t %b %b", $time, v0x55b677af2b80_0, v0x55b677af2c40_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55b677af2b80_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 100, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 20 "$random" 32 {0 0 0};
    %pad/u 4;
    %store/vec4 v0x55b677af2b80_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x55b677af2c40_0;
    %pad/u 32;
    %load/vec4 v0x55b677af2b80_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %mod;
    %cmp/ne;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x55b677af2b80_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %mod;
    %vpi_call 2 23 "$display", "Error: %b != %b", v0x55b677af2c40_0, S<0,vec4,u32> {1 0 0};
    %vpi_call 2 24 "$finish" {0 0 0};
T_0.2 ;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "./isodd.v";
