#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55abb72b3600 .scope module, "decode_tb" "decode_tb" 2 3;
 .timescale 0 0;
v0x55abb72c70b0_0 .var "din", 3 0;
v0x55abb72c7190_0 .net "even", 0 0, v0x55abb72c6db0_0;  1 drivers
v0x55abb72c7260_0 .net "odd", 0 0, v0x55abb72c6e70_0;  1 drivers
v0x55abb72c7360_0 .net "zero", 0 0, v0x55abb72c6f40_0;  1 drivers
S_0x55abb72b3780 .scope module, "dut" "decode" 2 8, 3 1 0, S_0x55abb72b3600;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "din"
    .port_info 1 /OUTPUT 1 "odd"
    .port_info 2 /OUTPUT 1 "even"
    .port_info 3 /OUTPUT 1 "zero"
v0x55abb727c100_0 .net "din", 3 0, v0x55abb72c70b0_0;  1 drivers
v0x55abb72c6db0_0 .var "even", 0 0;
v0x55abb72c6e70_0 .var "odd", 0 0;
v0x55abb72c6f40_0 .var "zero", 0 0;
E_0x55abb72b3f90 .event edge, v0x55abb727c100_0;
    .scope S_0x55abb72b3780;
T_0 ;
    %wait E_0x55abb72b3f90;
    %load/vec4 v0x55abb727c100_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 6;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55abb72c6f40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55abb72c6db0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55abb72c6e70_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55abb727c100_0;
    %parti/s 1, 0, 2;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.2, 6;
    %pushi/vec4 4, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x55abb72c6f40_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x55abb72c6db0_0, 0, 1;
    %store/vec4 v0x55abb72c6e70_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 2, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x55abb72c6f40_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x55abb72c6db0_0, 0, 1;
    %store/vec4 v0x55abb72c6e70_0, 0, 1;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55abb72b3600;
T_1 ;
    %vpi_call 2 16 "$monitor", "%d %b %b %b", v0x55abb72c70b0_0, v0x55abb72c7260_0, v0x55abb72c7190_0, v0x55abb72c7360_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55abb72c70b0_0, 0, 4;
    %delay 100, 0;
    %pushi/vec4 100, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 21 "$random" 32 {0 0 0};
    %pad/u 4;
    %store/vec4 v0x55abb72c70b0_0, 0, 4;
    %delay 100, 0;
    %load/vec4 v0x55abb72c7260_0;
    %load/vec4 v0x55abb72c70b0_0;
    %parti/s 1, 0, 2;
    %cmp/ne;
    %flag_mov 8, 4;
    %load/vec4 v0x55abb72c7190_0;
    %load/vec4 v0x55abb72c70b0_0;
    %parti/s 1, 0, 2;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55abb72c70b0_0;
    %parti/s 3, 1, 2;
    %or/r;
    %and;
    %cmp/ne;
    %flag_or 4, 8;
    %flag_mov 8, 4;
    %load/vec4 v0x55abb72c7360_0;
    %load/vec4 v0x55abb72c70b0_0;
    %or/r;
    %cmp/e;
    %flag_or 4, 8;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 24 "$display", "Error: %d %b %b %b", v0x55abb72c70b0_0, v0x55abb72c7260_0, v0x55abb72c7190_0, v0x55abb72c7360_0 {0 0 0};
    %vpi_call 2 25 "$finish" {0 0 0};
    %delay 100, 0;
T_1.2 ;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
